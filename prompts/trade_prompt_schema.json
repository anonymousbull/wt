{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TradePrompt",
  "description": "Configuration to initiate a swap",
  "type": "object",
  "properties": {
    "user_sk": {
      "description": "The user's secret key",
      "type": "string"
    },
    "sol_ui": {
      "description": "SOL amount",
      "type": "number",
      "format": "double"
    },
    "mint": {
      "description": "mint address",
      "type": "string"
    },
    "tp": {
      "description": "Take profit percent",
      "type": "number",
      "format": "double"
    },
    "sl": {
      "description": "Stop loss percent",
      "type": "number",
      "format": "double"
    }
  },
  "required": [
    "user_sk",
    "sol_ui",
    "mint",
    "tp",
    "sl"
  ]
}